PROJECT_NAME          = ctl
PROJECT_NUMBER        = 0.1
HIDE_UNDOC_MEMBERS    = NO
HIDE_UNDOC_CLASSES    = NO
SOURCE_BROWSER        = YES
@INCLUDE_PATH = @PROJECT_SOURCE_DIR@/ctl
INPUT = @PROJECT_SOURCE_DIR@/ctl
OUTPUT_DIRECTORY=@CMAKE_BINARY_DIR@/doc/doxygen
FILE_PATTERNS         = *.cpp *.hpp
EXCLUDE_SYMBOLS = ctl::detail::* detail::* std::*
GENERATE_XML = YES
RECURSIVE             = YES
EXCLUDE_PATTERNS      = *.asc *.cov *.nrv 
ALPHABETICAL_INDEX    = YES
COLS_IN_ALPHA_INDEX   = 3
IGNORE_PREFIX         = Q
ENABLE_PREPROCESSING  = YES
MACRO_EXPANSION       = YES
